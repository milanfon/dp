{
    "title": "spec 1 teacher",
    "steps": [
        {
            "type": "setViewport",
            "width": 1501,
            "height": 1104,
            "deviceScaleFactor": 1,
            "isMobile": false,
            "hasTouch": false,
            "isLandscape": false
        },
        {
            "type": "navigate",
